    <item>
      <title>CakePHP で Google &#43;1 ボタン を表示 (日本語で)</title>
      <link>https://copo.jp/posts/2011/cakephp_%E3%81%A7_google_&#43;1_%E3%83%9C%E3%82%BF%E3%83%B3_%E3%82%92%E8%A1%A8%E7%A4%BA_%E6%97%A5%E6%9C%AC%E8%AA%9E%E3%81%A7/</link>
      <pubDate>Wed, 29 Jun 2011 00:00:00 +0900</pubDate>
      
      <guid>https://copo.jp/posts/2011/cakephp_%E3%81%A7_google_&#43;1_%E3%83%9C%E3%82%BF%E3%83%B3_%E3%82%92%E8%A1%A8%E7%A4%BA_%E6%97%A5%E6%9C%AC%E8%AA%9E%E3%81%A7/</guid>
